{% embed "@atoms/layout/layout-site/_layout-site-main.twig" %}
    {% block header %}
        {% embed "@organisms/global/global-header/_global-header-main.twig" %}{% endembed %}
    {% endblock %}

    {% block main %}
        <div class="region--skils">
            <div class="container">
                {% include "@organisms/sections/section-stages/_section-stages-main.twig" with
                    {
                        "section_stages_data": stagesData,
                        "section_stages_classes": "mb-4"
                    }
                %}
                {% include "@organisms/sections/section-lists/_section-lists-main.twig" with
                    {
                        "section_lists_data": techniquesData
                    }
                %}
                <hr class="my-4 body_hr" style="color: white;">
                {% include "@organisms/sections/section-lists/_section-lists-main.twig" with
                    {
                        "section_lists_data": programsData,
                        "section_lists_title": "Programs & Software",
                        "section_lists_classes": "mb-4"
                    }
                %}
            </div>
        </div>
    {% endblock %}

    {% block footer %}
        {% include "@organisms/global/global-footer/_global-footer-main.twig" %}
    {% endblock %}
{% endembed %}