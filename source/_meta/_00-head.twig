    <link rel="icon" type="image/png" href="{{ path_to_assets }}assets/favicon/favicon-32x32.png" sizes="32x32" />
    <link rel="icon" type="image/png" href="{{ path_to_assets }}assets/favicon/favicon-16x16.png" sizes="16x16" />

    {% if prod_build == false %}
